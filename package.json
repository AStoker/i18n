{
  "name": "aurelia-i18n",
  "version": "0.2.3",
  "description": "A plugin that provides i18n support.",
  "keywords": [
    "aurelia",
    "i18n",
    "internationalization",
    "plugin"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia/i18n/issues"
  },
  "license": "MIT",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/aurelia/i18n"
  },
  "jspm": {
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "Intl.js": "github:andyearnshaw/Intl.js@^0.1.4",
      "aurelia-dependency-injection": "github:aurelia/dependency-injection@^0.10.1",
      "aurelia-event-aggregator": "github:aurelia/event-aggregator@^0.8.0",
      "aurelia-loader-default": "github:aurelia/loader-default@^0.10.0",
      "aurelia-templating": "github:aurelia/templating@^0.15.3",
      "i18next": "github:i18next/i18next@^1.9.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.6.4",
      "babel-runtime": "npm:babel-runtime@^5.6.4",
      "core-js": "npm:core-js@^0.9.17",
      "text": "github:systemjs/plugin-text@^0.0.2"
    }
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.3",
    "babel": "^5.8.23",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "babel-eslint": "^4.1.1",
    "gulp-babel": "^5.2.1",
    "gulp-bump": "^0.1.11",
    "gulp-eslint": "^1.0.0",
    "gulp-typedoc": "^1.2.1",
    "gulp-typedoc-extractor": "0.0.8",
    "jasmine-core": "^2.1.3",
    "karma": "^0.12.28",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.3.1",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-ajax": "git://github.com/martingust/karma-jasmine-ajax.git",
    "karma-jspm": "^2.0.1",
    "object.assign": "^1.0.3",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "vinyl": "^0.5.3",
    "vinyl-paths": "^1.0.0",
    "yargs": "^2.1.1"
  },
  "aurelia": {
    "usedBy": [],
    "documentation": {
      "links": [
        {
          "rel": "license",
          "mediaType": "text/plain",
          "title": "The MIT License (MIT)",
          "href": "LICENSE"
        },
        {
          "rel": "describedby",
          "mediaType": "application/aurelia-doc+json",
          "title": "API",
          "href": "doc/api.json"
        },
        {
          "rel": "version-history",
          "mediaType": "text/markdown",
          "title": "Change Log",
          "href": "doc/CHANGELOG.md"
        }
      ]
    }
  }
}
